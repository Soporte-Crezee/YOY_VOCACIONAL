    <add key="POVUrlOrientador" value="http://testpov.grupoplenum.com/PortalSocial/Auth/Login.aspx?u=ori"/>
    <add key="POVUrlPadres" value="http://testpov.grupoplenum.com/PortalTutor/Auth/Login.aspx"/>

    <!-- Configuracion de correo -->
    <add key="ConfigPath" value="Files\protocolo\config.txt"/>
    
    <!-- Templates-->
    <add key="POVUrlEmailTemplateNewUser" value="Files\protocolo\EmailTemplateNewUser.html"/>    

    <!-- Configuracion de ejecucion del servicio por dia-->
    <!-- Calculo en horas-->
    <!-- 30 minutos horas equivale 0.5-->
    <!-- 1 hora equivale a 1-->
    <!-- Para que el servicio se ejecuta cada hora value=1-->
    <add key="Servicio" value="1"/>
    <!--EJECUCION POR HORA-->

    <!-- Maximo de correo a enviar-->
    <add key="MaximoCorreos" value="100"/>
  </appSettings>
  <connectionStrings>
    <remove name="LocalSqlServer" />
    <remove name="ApplicationService" />
    <remove name="POV" />
