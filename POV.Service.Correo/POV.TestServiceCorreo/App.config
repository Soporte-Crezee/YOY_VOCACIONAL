<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!-- parametros framework Compañia -->
    <add key="POVDatabaseName" value="POV" />
    <add key="POVLikeSymbol" value="%" />
    <add key="POVParameterSymbol" value="@" />
    <add key="POVNamedParameters" value="true" />
    <add key="POVUserParameter" value="User ID" />
    <add key="POVPasswordParameter" value="Password" />

    <!-- Redes sociales -->
    <add key="POVUrlImgBackground" value="http://testpov.grupoplenum.com/images/header-yoy-template.png"/>
    <add key="POVUrlImgFace" value="http://testpov.grupoplenum.com/images/facebook.png"/>
    <add key="POVUrlImgTwit" value="http://testpov.grupoplenum.com/images/twitter.png"/>
    <add key="POVUrlImgLinked" value="http://testpov.grupoplenum.com/images/in.png"/>
    <add key="POVUrlImgInstagram" value="http://testpov.grupoplenum.com/images/instagram.png"/>

    <!-- Link Portales-->
